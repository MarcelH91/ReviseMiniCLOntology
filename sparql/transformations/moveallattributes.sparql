PREFIX clonto: <http://myCLOnto.de/> 
DELETE{ ?atset2 clonto:hasAttribute ?at . }
INSERT { ?atset1 clonto:hasAttribute ?at . }
WHERE{ 
  ?atset1 clonto:name ?atset1name .
  ?atset2 clonto:name ?atset2name .
  ?entity clonto:hasAttributeSet ?atset1 .
  ?entity clonto:hasAttributeSet ?atset2 .
  ?atset2 clonto:hasAttribute ?at . 
  ?at clonto:name ?atname .}